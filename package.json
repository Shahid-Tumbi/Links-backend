{
  "name": "links-backend",
  "version": "1.0.0",
  "description": "links project",
  "scripts": {
    "build": "tsc && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "start:fast": "NODE_ENV=production node ./dist/bin/server.js",
    "stag:fast": "NODE_ENV=staging node ./dist/bin/server.js",
    "test:fast": "NODE_ENV=testing node ./dist/bin/server.js",
    "dev:fast": "NODE_ENV=development node ./dist/bin/server.js",
    "local:fast": "npm run build && NODE_ENV=local node ./dist/bin/server.js",
    "start": "NODE_ENV=local ts-node -r tsconfig-paths/register ./src/bin/server.ts --fast --transpile-only",
    "stag": "NODE_ENV=staging ts-node -r tsconfig-paths/register ./src/bin/server.ts --fast --transpile-only",
    "test": "NODE_ENV=testing ts-node -r tsconfig-paths/register ./src/bin/server.ts --fast --transpile-only",
    "dev": "NODE_ENV=development ts-node -r tsconfig-paths/register ./src/bin/server.ts --fast --transpile-only",
    "local-dev": "NODE_ENV=local ts-node -r tsconfig-paths/register ./src/bin/server.ts --fast --transpile-only",
    "local": "NODE_ENV=local ts-node-dev --poll -r tsconfig-paths/register ./src/bin/server.ts",
    "lint": "tslint -p tsconfig.json"
  },
  "repository": "https://github.com/Shahid-Tumbi/Links-backend.git",
  "author": "",
  "license": "ISC",
